ctrl - f : yabai -m window --toggle zoom-fullscreen

# remove windows from tiling
ctrl - t : yabai -m window --toggle float;\
  yabai -m window --grid 4:4:1:1:2:2

# lock screen immediately
#cmd - escape : open -a ScreenSaverEngine

# change window focus within space with vimish keybindings
ctrl - j : window --focus west
ctrl - k : window --focus south
ctrl - l : window --focus north
ctrl - ; : window --focus east
